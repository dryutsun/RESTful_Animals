
<form method="GET" action="/prehistoric_creatures">
    <label for="typeFilter">Filter by Name</label>
    <input id="typeFilter" type="text" name="typeFilter">
    <input type="submit">
  </form>

    
    
    <%prehistoricData.forEach((animal, index)=>{%>
        <li>is a <%= animal.type %></li>
        <form action="/dinosaurs/<%= index %>/?_method=DELETE" method="POST"> 
            <input type="submit" value="delete">
        </form>
        </li>
    <% }) %>
