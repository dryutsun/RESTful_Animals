<form action="/dinosaurs" method="post"> 
    <!-- METHOD is the HTTP VERB and ACTION is telling what route to send it to.-->
    <!-- The Key gets sent as the name attr of the input tag -->
    <!-- We need to coordinate the HTML tags with how it will be sent as an OBJ -->
    <label for="dinosaurName">Name</label>
    <input type="text" id="dinosaurName" name="name">
    <label for="dinosaurType">Type</label>
    <input type="text" id="dinosaurType" name="type">

    <input type="submit" value="Add a Dino">
</form>